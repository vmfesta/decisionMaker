<body>

  <!-- Top nav bar (fixed) -->
  <nav id="nav-bar">
    <h1 class="header">Decision Maker</h1>
  </nav>

  <!-- Page-specific (main) content here -->
  <div class="container">
    <div class="row text-center">
      <header class="Poll">
        <h1 class="dTitle" id="<%=url%>">Go Vote</h1>
      </header>
      <span class="Poll-descrip">
        <h6 class ="Description">Instructions: To vote simply drag your most preferred choices from top to bottom</h6>
      </span>
    </div>

    <div class="row">
      <div class="col-md-6">
        <div class="Options text-center"><b>Options</b></div>
        <ul class="poll-list">
          <div id="content" style="height: 150px;">
            <div class="draggable" id="1">
              <div id="op1" class="item"><b>Option 1</b></div>
              <p id="desc1" class="description">Description of Option</p>
            </div>
          </div>
          <div id="content" style="height: 150px;">
            <div class="draggable" id="2">
              <div id="op2" class="item "><b>Option 2</b></div>
              <p id="desc2" class="description">Description of Option</p>
            </div>
          </div>
          <div id="content" style="height: 150px;">
            <div class="draggable" id="3">
              <div id="op3" class="item"><b>Option 3</b></div>
              <p id="desc3" class="description">Description of Option</p>
            </div>
          </div>
        </ul>
      </div>
      <div class="col-md-6">
        <ul id="votes" class="votes">
          <div class="Votetitle text-center"><b>Votes</b></div>
          <div id="content" style="height: 150px;">
            <div class="droppable">
              <div class="number text-center">1</div>
            </div>
          </div>
          <div id="content" style="height: 150px;">
            <div class="droppable">
              <div class="number text-center">2</div>
            </div>
          </div>
          <div id="content" style="height: 150px;">
            <div class="droppable">
              <div class="number text-center">3</div>
            </div>
            <div class="button">
              <button id="votebutton" type="submit"><b>Submit Vote</b></button>
            </div>
            <div>
            </div>
        </ul>
        </div>


